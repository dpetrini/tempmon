    "express": "^4.15.3",
    "method-override": "^2.3.9",
    "mongojs": "^2.4.0",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0"
  },
  "devDependencies": {
    "supertest": "^3.0.0"
  }
