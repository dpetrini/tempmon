{
  "name": "tempmon",
  "version": "0.1.4",
  "private": true,
  "description": "app express mongo ",
  "main": "./bin/www.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dpetrini/tempmon.git"
  },
  "scripts": {
    "start": "node ./bin/www.js",
    "pretest": "jshint *",
